{
  "name": "postcss-custom-properties-fallback",
  "description": "Adds fallbacks to your CSS var() functions",
  "license": "MIT",
  "author": "Cody Olsen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stipsan/postcss-custom-properties-fallback.git"
  },
  "bugs": {
    "url": "https://github.com/stipsan/postcss-custom-properties-fallback/issues"
  },
  "version": "1.0.1",
  "main": "dist/plugin.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "microbundle ./src/* --target node -f cjs",
    "prepare": "npm run -s build",
    "test": "tap test/*.js --no-coverage",
    "test:snapshot": "TAP_SNAPSHOT=1 tap test/*.js --no-coverage"
  },
  "dependencies": {
    "postcss-value-parser": "^4.1.0",
    "postcss-values-parser": "^4.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "microbundle": "^0.13.0",
    "postcss": "^8.1.8",
    "tap": "^14.10.8"
  },
  "keywords": [
    "css",
    "csswg",
    "custom",
    "declarations",
    "postcss",
    "postcss-plugin",
    "properties",
    "specification",
    "variables",
    "vars",
    "w3c"
  ],
  "prettier": {
    "singleQuote": true
  }
}
